<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="481.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.bankSystem.controller.RegisterFormController">
   <children>
      <TextField fx:id="txtFullName" layoutX="103.0" layoutY="54.0" prefHeight="31.0" prefWidth="381.0" promptText="Full Name" />
      <TextField fx:id="txtAccountNumber" editable="false" layoutX="103.0" layoutY="411.0" prefHeight="31.0" prefWidth="381.0" promptText="Account Number" />
      <TextField fx:id="txtUserName" layoutX="103.0" layoutY="100.0" prefHeight="31.0" prefWidth="381.0" promptText="User Name" />
      <TextField fx:id="txtPass" layoutX="103.0" layoutY="169.0" prefHeight="31.0" prefWidth="381.0" promptText="Password" />
      <TextField fx:id="txtConfPass" layoutX="103.0" layoutY="227.0" prefHeight="31.0" prefWidth="381.0" promptText="Confirm Password" />
      <Button fx:id="btnRegister" layoutX="367.0" layoutY="294.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" prefHeight="31.0" prefWidth="117.0" text="Register" />
      <Label layoutX="103.0" layoutY="297.0" onMouseClicked="#lblOnAction" text="Already Have an Account ?" textFill="#1f39e1">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="lblUserName" layoutX="103.0" layoutY="131.0" prefHeight="21.0" prefWidth="381.0" text="User Name Already Exist!" textFill="#f81010">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
      <Button fx:id="btnLogin" layoutX="367.0" layoutY="351.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="31.0" prefWidth="117.0" text="Login" />
   </children>
</AnchorPane>
